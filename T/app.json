	"scripts": {
		"449AB29E-5281-4B62-9771-2E6E57D32494": [
			{
				"name": "",
				"source": "var group = new THREE.Group();\ngroup.rotation.x = - 0.75;\nscene.add( group );\n\nvar background = scene.getObjectByName( 'Background' );\ngroup.add( background );\n\n// Box\n\nvar slice = scene.getObjectByName( 'Slice' );\nscene.remove( slice );\n\nvar box = new THREE.Group();\ngroup.add( box );\n\nfor ( var i = 0; i < 20; i ++ ) {\n\n\tvar clone = slice.clone();\n\tclone.material = slice.material.clone();\n\tclone.position.y = i * 5 - 50;\n\tbox.add( clone );\n\t\n}\n\nfunction update( event ) {\n\n\tvar time = event.time * 0.0003;\n\t\n\tgroup.rotation.x = time;\n\t\n\tfor ( var i = 0, l = box.children.length; i < l; i ++ ) {\n\n\t\tvar child = box.children[ i ];\n\t\tchild.rotation.y = Math.cos( time * 2 + i * 0.1 ) * Math.sin( time );\n\t\tchild.material.emissive.r = Math.pow( Math.abs( child.rotation.y * 0.9 ), 3 );\n\t\n\t}\n\n}"
			}]
